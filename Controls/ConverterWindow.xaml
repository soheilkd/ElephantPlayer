<Window x:Class="Player.Controls.ConverterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Player.Controls" WindowStartupLocation="CenterScreen"
        mc:Ignorable="d" Height="100" Width="600"
        Title="ConverterWindow" >
    <Grid>
        <ProgressBar x:Name="ProgressBar" Height="30" VerticalAlignment="Top"/>
        <Button Content="Cancel" HorizontalAlignment="Right" Margin="0,35,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label x:Name="Label1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,33,0,0" Content="Progress Logging"/>
    </Grid>
</Window>
