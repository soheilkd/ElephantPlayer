<Window x:Class="Player.Controls.ConverterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Player.Controls" WindowStartupLocation="CenterScreen"
        mc:Ignorable="d" Height="105" Width="600" x:Name="window" Closing="Window_Closing"
        Title="ConverterWindow" >
	<Grid>
		<ProgressBar x:Name="ProgressBar" Height="20" VerticalAlignment="Top"/>
		<Label x:Name="Label1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,23,0,0" Content="Progress Logging"/>
		<Expander x:Name="Expander1" VerticalAlignment="Bottom" IsExpanded="False">
			<Expander.Header>
				<TextBlock Text="Log" FontSize="14"/>
			</Expander.Header>
			<Expander.Content>
				<TextBox x:Name="TextBox1" MinLines="10" Height="160" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto"  MaxLines="10000"/>
			</Expander.Content>
		</Expander>
	</Grid>
</Window>
