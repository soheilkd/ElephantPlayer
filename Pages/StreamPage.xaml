<Page
    x:Class="Player.Pages.StreamPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" NavigationCacheMode="Required">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>
		<AppBar>
				<StackPanel Orientation="Horizontal" Spacing="5">
					<Button Content="Youtube" Click="Button_Click"/>
					<Button Content="Soundcloud" Click="Button_Click_1"/>
				</StackPanel>
			</AppBar>
		<WebView x:Name="MainWebView" Grid.Row="1"/>
	</Grid>
</Page>
